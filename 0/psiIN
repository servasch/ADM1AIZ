FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      psiIN;
}
// * Inorganic Nitrogen * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 103; //mol/m3

boundaryField
{
    inlet
    {
	type	groovyBC;
	valueExpression "psiINOutlet";
	variables "psiINOutlet{outlet}=average(psiIN);";
	value uniform 70;

    }

    outlet
    {
        type            fixedValue;
        value           uniform 70;
    }

    wall
    {
        type            zeroGradient;
    }

    wall-tank
    {
        type            zeroGradient;
    }

    bot
    {
        type            zeroGradient;
    }


    frontAndBackPlanes
    {
        type            empty;
    }
}

