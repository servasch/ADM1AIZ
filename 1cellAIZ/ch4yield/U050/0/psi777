FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      psi777;
}
// * Volatile Fatty Acids- propionate * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0.0001; //kg/m3

boundaryField
{
    inlet
    {
	type	groovyBC;
	valueExpression "psi777Outlet";
	variables "psi777Outlet{outlet}=average(psi777);";
	value uniform 0.001;
    }

    outlet
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }


    frontAndBack
    {
        type            empty;
    }
}

