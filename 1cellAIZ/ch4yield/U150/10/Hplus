FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "10";
    object      Hplus;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0.0001;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.0001;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0.0001;
    }
    fixedWalls
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


